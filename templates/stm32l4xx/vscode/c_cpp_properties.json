{
    "env": {
        "myIncludePath": [
            "${workspaceFolder}/project/**"
        ],
        "myDefines": [
            "__VFP_FP__"
        ],
        "autogeneratedDefines": [
            "STM32L412xx",
            "CORE_CM4"
        ]
    },
    "configurations": [
        {
            "name": "pyCProject",
            "includePath": [
                "${myIncludePath}"
            ],
            "defines": [
                "${myDefines}",
                "${autogeneratedDefines}"
            ],
            "cStandard": "gnu11",
            "cppStandard": "gnu++14",
            "browse": {
                "path": [
                    "${myIncludePath}"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}